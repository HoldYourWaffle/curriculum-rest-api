        id
        title
        Vak {
          id
          title
        }
      }
      Vaksubkern {
        id
        title
        Vakkern {
          id
          title
          Vak {
            id
            title
          }
        }
      }
      Vakinhoud {
        id
        title
        Vaksubkern {
          id
          title
          Vakkern {
            id
            title
            Vak {
              id
              title
            }
